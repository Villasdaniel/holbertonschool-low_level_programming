#include "holberton.h"
#include <stdio.h>

/**
 * main -  prints the number of arguments
 * @argc: number of elements string
 * @argv: elements string
 * Return: 0 if everything is correct
 */

int main(int argc, char *argv[]__attribute__((unused)))
{
	int argc1;
	
	argc1 = argc + 1;
	printf("%d\n", argc1);
	return (0);
}

